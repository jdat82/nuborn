{
	"uglify": {
		"compress": {
			"global_defs": {
				"debug": false,
				"livereload": false,
				"weinre": false
			},
			"dead_code": true,
			"drop_debugger": true
		},
		"beautify": false,
		"report": "min",
		"warnings": true,
		"mangle": true,
		"banner": "/**\n * Â© Jean Dat 2013\n * Generated on <%=grunt.template.today()%>\n */\n"
	},
	"sass": {
		"style": "compressed",
		"noCache": true,
		"compass": true
	},
	"html": {
		"removeComments": true,
		"collapseWhitespace": true,
		"collapseBooleanAttributes": true
	}
}