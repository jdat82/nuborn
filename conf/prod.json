{
	"uglify": {
		"compress": {
			"global_defs": {
				"debug": false
			},
			"dead_code": true
		},
		"beautify": false,
		"report": "min",
		"warnings": true,
		"mangle": true
	},
	"sass": {
		"style": "compressed",
		"noCache": true
	},
	"html": {
		"removeComments": true,
		"collapseWhitespace": true,
		"collapseBooleanAttributes": true
	}
}