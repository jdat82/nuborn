{
	"uglify": {
		"compress": {
			"global_defs": {
				"DEBUG": false,
				"INFO": true,
				"WARN": true,
				"ERROR": true,
				"LIVERELOAD": false,
				"LIVERELOAD_PORT": 35735,
				"WEINRE": false,
				"WEINRE_PORT": 8085,
				"NETWORK": "http://<HOST>:<PORT>"
			},
			"dead_code": true,
			"drop_debugger": true
		},
		"beautify": false,
		"report": "min",
		"warnings": true,
		"mangle": true,
		"banner": "/**\n * Â© Jean Dat 2013\n * Generated on <%=grunt.template.today()%>\n */\n"
	},
	"sass": {
		"style": "compressed",
		"noCache": true,
		"compass": true
	},
	"html": {
		"removeComments": true,
		"collapseWhitespace": true,
		"collapseBooleanAttributes": true
	}
}