{
	"uglify": {
		"compress": {
			"global_defs": {
				"debug": true
			},
			"dead_code": true
		},
		"beautify": true,
		"report": "false",
		"warnings": true,
		"mangle": false
	},
	"sass": {
		"style": "expanded",
		"noCache": true
	},
	"html": {
		"removeComments": false,
		"collapseWhitespace": false,
		"collapseBooleanAttributes": false
	}
}